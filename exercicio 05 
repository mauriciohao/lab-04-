#include <stdio.h>
#include <stdbool.h>

// exercicio 5 - a)
bool primo(int x) {
    if (x <= 1) {
        return false;
    }
    for (int i = 2; i * i <= x; i++) {
        if (x % i == 0) {
            return false;
        }
    }
    return true;
}
// b)
int main(){
    int n1, n2;
    printf("Digite o primeiro numero:");
    scanf("%d", &n1);
    printf("Digite o segundo numero:");
    scanf("%d", &n2);

    printf("NÃºmeros primos entre %d e %d:\n", n1, n2);
    for (int i = n1; i <= n2; i++) {
        if (primo(i)) {
            printf("%d,", i);
        }
    }
    return 0;
  }